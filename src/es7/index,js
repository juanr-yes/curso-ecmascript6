//////// Que se implement√≥ en ES7?
//// .includes para strings y arreglos, anteriormente se ocupaba indexOf

let numbers = [1, 2, 3, 7, 8];

if (numbers.includes(7)) {
  console.log("Si se encuentra el valor 7");
} else {
  console.log("No se encuentra el valor 7");
}

//// doble asterisco (**) para elevar a la potencia

let base = 4;
let exponent = 3;
let result = base**exponent;

console.log(result);